{
  "compilerOptions": {
    "target": "es5",
    "module": "ES6",
    "sourceMap": true,
    "lib": ["es2016", "DOM"],
    "outDir": "build",
    "downlevelIteration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "exclude": [
    "src/lib",
    "node_modules"
  ]
}